DROP TABLE IF EXISTS `friendRequests`;
CREATE TABLE `friendRequests` (
  `id_orig` int NOT NULL,
  `id_dest` int NOT NULL,
  `state` int DEFAULT '0',
  `accept_time` timestamp NULL DEFAULT NULL,
  PRIMARY KEY (`id_orig`,`id_dest`),
  KEY `id_dest` (`id_dest`),
  CONSTRAINT `friendRequests_ibfk_1` FOREIGN KEY (`id_orig`) REFERENCES `users` (`id`),
  CONSTRAINT `friendRequests_ibfk_2` FOREIGN KEY (`id_dest`) REFERENCES `users` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;

